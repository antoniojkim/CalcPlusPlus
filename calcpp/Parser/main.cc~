#include <cassert>
#include <charconv>
#include <exception>
#include <iostream>
#include <string>
#include <string_view>
#include <system_error>
#include <vector>

#include "../Utils/MultiStack.h"

using namespace std;
using namespace calcpp;

int main() {
    const char* c = "123.54";
    string_view s(c, 6);
    cout << s << endl;
    double num;
    const auto result = std::from_chars(
        (const char*) s.data(),
        (const char*) s.data() + s.size(),
        num,
        std::chars_format::general);
    if (result.ec == std::errc::invalid_argument) {
        cout << "Failed to convert" << endl;
    } else {
        cout << num << endl;
    }
}
